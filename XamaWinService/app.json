{
    "enableLog": true,
    "logConfig": {
        "fileName": "teste.log",
        "path": "c:/temp/",
        "maxSize": 50000000,
        "maxFiles": 5,
        "showTrace": false
    },
    "tasks": [
        {
            "name": "jobTest",
            "compressionLevel": 9,
            "paths": [
                {
                    "path": "c:/Desenv",
                    "includesSubfolders": true,
                    "excludes": [
                        {
                            "pattern": "bin",
                            "applyesTo": "Directory"
                        },
                        {
                            "pattern": "obj",
                            "applyesTo": "Directory"
                        },
                        {
                            "pattern": "node_modules",
                            "applyesTo": "Directory"
                        },
                        {
                            "pattern": "dbs",
                            "applyesTo": "Directory"
                        },
                        {
                            "pattern": "gps",
                            "applyesTo": "Directory"
                        },
                        {
                            "pattern": "packages",
                            "applyesTo": "Directory"
                        },
                        {
                            "pattern": ".*"
                        },
                        {
                            "pattern": "*.r",
                            "applyesTo": "File"
                        }
                    ]
                }
            ],
            "target": {
                "fileName": "minhacopia2.7zip",
                "path": "c:/temp/"
            }
        }
    ]
}