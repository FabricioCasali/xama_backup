{
    "enableLog": true,
    "logConfig": {
        "fileName": "teste.log",
        "path": "c:/temp/",
        "maxSize": 50000000,
        "maxFiles": 5,
        "showTrace": false
    },
    "tasks": [
        {
            "name": "jobTest",
            "paths": [
                {
                    "path": "c:/Desenv",
                    "includesSubfolders": true,
                    "excludes": [
                        {
                            "pattern": "bin",
                            "appliesTo": "directory"
                        },
                        {
                            "pattern": "obj",
                            "appliesTo": "Directory"
                        },
                        {
                            "pattern": "node_modules",
                            "appliesTo": "Directory"
                        },
                        {
                            "pattern": "dbs",
                            "appliesTo": "Directory"
                        },
                        {
                            "pattern": "gps",
                            "appliesTo": "Directory"
                        },
                        {
                            "pattern": "packages",
                            "appliesTo": "Directory"
                        },
                        {
                            "pattern": ".*"
                        },
                        {
                            "pattern": "*.r",
                            "appliesTo": "File"
                        }
                    ]
                }
            ],
            "schedules": [
                {
                    "type": "ConfigScheduleDaily",
                    "time": "12:00:00"
                },
                {
                    "type": "ConfigScheduleMonthly",
                    "time": "12:00:00",
                    "day": 15
                }
            ],
            "target": {
                "fileName": "ultra",
                "path": "c:/temp/",
                "compressionMethod": "Zip",
                "compressionLevel": "Ultra"
            }
        }
    ]
}